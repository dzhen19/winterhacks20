{
  "name": "kapwing-interview",
  "version": "1.0.0",
  "description": "This repo contains code for the Kapwing technical interview project.",
  "scripts": {
    "build": "npm install && (cd client/ && npm install && npm run build)",
    "start-client": "(cd client/ && npm run start)",
    "start-server": "(cd server/ && FLASK_APP=index.py FLASK_DEBUG=1 flask run)",
    "start": "concurrently 'npm run start-client' 'npm run start-server'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kapwing/interview.git"
  },
  "engines": {
    "node": "^12"
  },
  "heroku-run-build-script": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "react-dropzone": "^11.2.4"
  }
}
